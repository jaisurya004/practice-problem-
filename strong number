#include<stdio.h>
#include<math.h>
int main(){
    int a,b,c,d,i,p=1,s=0;
    scanf("%d",&a);
    b=a;
    while(b!=0){
        d=b%10;
        for(int j=1;j<=d;j++){
            p=p*j;
        }
        b=b/10;
        s=s+p;
        p=1;
    }
    if(a==s)
    printf("it is a strong number");
    else
    printf("it is not a strong number");
}
